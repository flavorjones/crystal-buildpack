#!/bin/bash
set -euo pipefail

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
DEPS_IDX=$4
PROFILE_DIR=${5:-}

echo "BUILD_DIR: ${BUILD_DIR}"
echo "CACHE_DIR: ${CACHE_DIR}"
echo "DEPS_DIR: ${DEPS_DIR}"
echo "DEPS_IDX: ${DEPS_IDX}"
echo "PROFILE_DIR: ${PROFILE_DIR}"

crystal ${BASH_SOURCE%/*}/finalize.cr $BUILD_DIR $CACHE_DIR
